root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
indent_style = space
indent_size = 4
end_of_line = lf

[*.{csproj,sln}]
indent_style = space
indent_size = 2
end_of_line = lf

[*.{xml,json}]
indent_style = space
indent_size = 2
end_of_line = lf
